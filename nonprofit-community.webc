<div class="py-6 sm:py-12 px-4 sm:px-6 lg:py-16 lg:px-8 mb-36">
    <div class="mx-auto max-w-7xl">
        <h3 class="text-2xl font-extrabold text-gray-900 tracking-tight sm:text-3xl">
            You believe technology can amplify your impact.
        </h3>
        <p class="mt-3 text-lg text-silver-700">
            Learn and grow with people who feel the same way. 
        </p>
        <div class="mt-3 flex items-center gap-x-6">
            <button @size="lg" @color="green" class="mt-8" href="/application">
            Apply Today
            </button>
            <button @size="lg" @color="onyx" class="mt-8" href="#missions">
            Learn More <span aria-hidden="true"> ↓</span>
            </button>
        </div>
    </div>
</div>


<!-- standard footer CTA's -->
<div id="missions">
    <cta-missions @label="Nonprofits" :@missions="collections.missions.filter(mission => mission.data.tags.includes('nonprofit')).slice(-3).reverse()"></cta-missions>
    <cta-meeting :@team="true"></cta-meeting>
</div>
